{
  "schema": "schema.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "name": "TestClient",
      "namespace": "Demo.GraphQL",
      "url": "https://localhost:5001/graphql/",
      "records": {
        "inputs": true,
        "entities": true
      },
      "transportProfiles": [
